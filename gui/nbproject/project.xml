<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>QuantoGui</name>
            <properties/>
            <folders>
                <source-folder>
                    <label>src</label>
                    <type>java</type>
                    <location>src</location>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>compile</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="run">
                    <target>run</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>compile</target>
                </action>
                <action name="debug">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>debug-nb</target>
                </action>
                <action name="compile.single">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>compile-selected-files-in-src</target>
                    <context>
                        <property>files</property>
                        <folder>src</folder>
                        <pattern>\.java$</pattern>
                        <format>relative-path</format>
                        <arity>
                            <separated-files>,</separated-files>
                        </arity>
                    </context>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>build</location>
                <build-target>compile</build-target>
            </export>
            <export>
                <type>jar</type>
                <location>dist/QuantoGui.jar</location>
                <build-target>compile</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>src</label>
                        <location>src</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="run"/>
                    <ide-action name="debug"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>src</package-root>
                <classpath mode="compile">src:lib/iText-5.0.6.jar:lib/collections-generic-4.01.jar:lib/colt-1.2.0.jar:lib/jung-algorithms-2.0.1.jar:lib/jung-api-2.0.1.jar:lib/jung-graph-impl-2.0.1.jar:lib/jung-visualization-2.0.1.jar:lib/nanoxml-2.2.3.jar:lib/slf4j-api-1.6.1.jar:../../java-sources/jung-algorithms-2.0.1-sources.jar:../../java-sources/jung-api-2.0.1-sources.jar:../../java-sources/jung-graph-impl-2.0.1-sources.jar:../../java-sources/jung-visualization-2.0.1-sources.jar:../../java-sources/slf4j-1.6.1/slf4j-api-1.6.1-sources.jar:../../java-sources/iText/core:../../java-sources/colt/src</classpath>
                <built-to>build</built-to>
                <built-to>dist/QuantoGui.jar</built-to>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
        <preferences xmlns="http://www.netbeans.org/ns/auxiliary-configuration-preferences/1">
            <module name="org-netbeans-modules-editor-indent">
                <node name="CodeStyle">
                    <property name="usedProfile" value="project"/>
                    <node name="project">
                        <property name="spaces-per-tab" value="8"/>
                        <property name="tab-size" value="8"/>
                        <property name="indent-shift-width" value="8"/>
                        <property name="text-limit-width" value="80"/>
                        <property name="expand-tabs" value="false"/>
                    </node>
                </node>
                <node name="text">
                    <node name="x-java">
                        <node name="CodeStyle">
                            <node name="project">
                                <property name="indent-shift-width" value="8"/>
                                <property name="placeWhileOnNewLine" value="true"/>
                                <property name="text-limit-width" value="80"/>
                                <property name="alignMultilineThrows" value="true"/>
                                <property name="alignMultilineTernaryOp" value="true"/>
                                <property name="spaces-per-tab" value="8"/>
                                <property name="alignMultilineMethodParams" value="true"/>
                                <property name="placeElseOnNewLine" value="true"/>
                                <property name="alignMultilineCallArgs" value="true"/>
                                <property name="tab-size" value="8"/>
                                <property name="placeCatchOnNewLine" value="true"/>
                                <property name="placeFinallyOnNewLine" value="true"/>
                                <property name="expand-tabs" value="false"/>
                            </node>
                        </node>
                    </node>
                </node>
            </module>
            <module name="org-netbeans-modules-projectimport-eclipse-core"/>
        </preferences>
    </configuration>
</project>
