  keywords = {quant-ph, cs.LO, math.CT, math-ph, math.MP},
  local-url = {file://localhost/Users/aleks/Documents/Papers/Abramsky%20-%20A%20categorical%20semantics%20of%20quantum%202004.pdf},
  pmid = {quant-ph/0402130v5},
